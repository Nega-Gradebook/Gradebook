<p id="notice"><%= notice %></p>

<h1>Listing Parents</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parent.each do |parent| %>
      <tr>
        <td><%= parent.name %></td>
        <td><%= parent.email %></td>
        <td><%= parent.student.name%></td>
        <td><%= link_to 'Show', parent %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div><%= link_to 'Dashboard', dashboard_index_path%></div>


<p><%= link_to 'Log Out', logout_path, method: :delete%></p>
